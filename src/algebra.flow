import opers/array;
import opers/bool;
import opers/int;
import opers/fol;
import opers/lambda;
import opers/string;
import opers/control;
import opers/tuple;
import opers/reflection;
import opers/map;

export {
    fullAlgebra : EvalAlgebra;
    allOpers : [EvalOper];
    allConsts : [EvalConst];
}

fullAlgebra = EvalAlgebra("Algebra", allOpers, allConsts);

allOpers = concatA([
    arrayOpers,
    boolOpers,
    intOpers,
    folOpers,
    stringOpers,
	lambdaOpers,
	controlOpers,
	tupleOpers,
	reflectionOpers,
	mapOpers,
]);

allConsts = [
    intConst,
    stringConst
];
